<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.study.customview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ui.ColorActivity">
        </activity>
        <activity android:name=".ui.PointActivity">
        </activity>
        <activity android:name=".ui.RectActivity">
        </activity>
        <activity android:name=".ui.LineActivity">
        </activity>
        <activity android:name=".ui.RoundRectActivity">
        </activity>
        <activity android:name=".ui.OvalActivity">
        </activity>
        <activity android:name=".ui.CircleActivity">
        </activity>
        <activity android:name=".ui.ArcActivity">
        </activity>
        <activity android:name=".ui.CircularActivity">
        </activity>
    </application>

</manifest>